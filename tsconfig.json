{
    "compilerOptions": {
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react-jsx",
        "module": "esnext",
        "moduleResolution": "node",
        "noUncheckedIndexedAccess": true,
        "skipLibCheck": true,
        "sourceMap": true
    },
    "ts-node": {
        "compilerOptions": { "module": "commonjs", "esModuleInterop": true }
    }
}
